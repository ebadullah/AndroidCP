                    <View style="@style/EView" />

                    <EditText
                        android:id="@+id/dna8bTv"
                        style="@style/SResponce"
                        android:hint="@string/dna8b"
                        android:inputType="number" />

                    <View style="@style/EView" />

                </LinearLayout>


                <LinearLayout style="@style/nLinerLayout">

                    <TextView
                        style="@style/SLable"
                        android:text="@string/dna9L"
                        android:textColor="@color/black"

                        />

                </LinearLayout>

                <!--demage to property  starts -->
                <LinearLayout style="@style/nLinerLayout">

                    <LinearLayout style="@style/nLinerLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/dna9"

                            />

                        <RadioGroup
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <RadioButton
                                android:id="@+id/dna9a"
                                style="@style/SResponce"
                                android:text="@string/dna9a" />

                            <RadioButton
                                android:id="@+id/dna9b"
                                style="@style/SResponce"
                                android:text="@string/dna9b" />

                            <RadioButton
                                android:id="@+id/dna9c"
                                style="@style/SResponce"
                                android:text="@string/dna9c" />

                        </RadioGroup>


                    </LinearLayout>

                    <LinearLayout style="@style/nLinerLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/dna10"

                            />

                        <RadioGroup
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <RadioButton
                                android:id="@+id/dna10a"
                                style="@style/SResponce"
                                android:text="@string/dna10a" />

                            <RadioButton
                                android:id="@+id/dna10b"
                                style="@style/SResponce"
                                android:text="@string/dna10b" />

                        </RadioGroup>


                    </LinearLayout>


                    <LinearLayout style="@style/nLinerLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/dna11"

                            />

                        <RadioGroup
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <RadioButton
                                android:id="@+id/dna11a"
