                        <CheckBox
                            android:id="@+id/dna13a"
                            style="@style/SResponce"
                            android:text="@string/dna13a" />

                        <CheckBox
                            android:id="@+id/dna13b"
                            style="@style/SResponce"
                            android:text="@string/dna13b" />

                        <CheckBox
                            android:id="@+id/dna13c"
                            style="@style/SResponce"
                            android:text="@string/dna13c" />

                        <EditText
                            android:id="@+id/dna13d"
                            style="@style/EResponce"
                            android:layout_marginStart="10dp"

                            android:layout_marginEnd="10dp"
                            android:hint="@string/dna13d"

                            />

                        <View
                            style="@style/nView"

                            android:layout_marginStart="10dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="15dp"


                            />


                    </LinearLayout>


                    <LinearLayout
                        style="@style/nLinerLayout"
                        android:visibility="gone">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/dna14"

                            />

                        <EditText
                            android:id="@+id/dna14a"
                            style="@style/SResponce"
                            android:hint="@string/dna14a"
                            android:inputType="number" />

                        <View style="@style/EView" />

                        <EditText
                            android:id="@+id/dna14b"
                            style="@style/SResponce"
                            android:hint="@string/dna14b"
                            android:inputType="number" />

                        <View style="@style/EView" />

                        <EditText
                            android:id="@+id/dna14c"
                            style="@style/SResponce"
                            android:hint="@string/dna14c"
                            android:inputType="number" />


                        <View style="@style/EView" />


                    </LinearLayout>

                </LinearLayout>

                <LinearLayout style="@style/nLinerLayout">

                    <TextView
                        style="@style/SLable"
                        android:hint="@string/dna15"


                        />

                    <EditText
                        android:id="@+id/dna15"
                        style="@style/EResponce"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        android:hint="@string/dna15"

                        android:inputType="text"
                        android:lines="3"

                        />

                    <View
                        style="@style/nView"

                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginBottom="15dp"


                        />

                    <View style="@style/nView" />

                </LinearLayout>
                <!--demage to property  ends -->


                <LinearLayout style="@style/nLinerLayout">


                    <LinearLayout style="@style/StaricLinLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/s_g_crnt_loc"

                            />

                        <TextView style="@style/StaricTextview" />
                    </LinearLayout>

                    <CheckBox
                        android:id="@+id/checkboxGps"

                        style="@style/Undertaking"
                        android:text="@string/undertakingGPS"

                        />


                    <LinearLayout style="@style/IconLinLayout"

                        >

                        <TextView
                            style="@style/SResponce"
                            android:layout_width="0dp"
                            android:layout_weight="30"
                            android:hint="@string/s_g_lat" />

                        <TextView
                            android:id="@+id/latitude"
                            style="@style/SResponce"

                            android:layout_width="0dp"
                            android:layout_weight="70" />


                    </LinearLayout>

                    <LinearLayout style="@style/IconLinLayout"

                        >

                        <TextView
                            style="@style/SResponce"
                            android:layout_width="0dp"
                            android:layout_weight="30"
                            android:hint="@string/s_g_lon"

                            />

                        <TextView
                            android:id="@+id/longitude"
                            style="@style/SResponce"

                            android:layout_width="0dp"
                            android:layout_gravity="start"
                            android:layout_weight="70" />


                    </LinearLayout>

                    <View style="@style/nView" />

                </LinearLayout>

                <LinearLayout style="@style/nLinerLayout">


                    <LinearLayout style="@style/StaricLinLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/s_g_h_dec"

                            />

                        <TextView style="@style/StaricTextview" />
                    </LinearLayout>

                    <CheckBox
                        android:id="@+id/checkbox"
                        style="@style/Undertaking" />

                    <View style="@style/nView" />

                </LinearLayout>

                <LinearLayout style="@style/nLinerLayout">

                    <TextView
                        style="@style/SLable"
                        android:text="@string/s_g_h_note"

                        />

                    <TextView
                        style="@style/note"
                        android:text="@string/s_g_note"

                        />

                    <Button
                        android:id="@+id/btnsubmit"
                        style="@style/SubmiteButt" />


                </LinearLayout>

            </LinearLayout>


        </ScrollView>


