<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="MissingDefaultResource">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="8dp"
        android:orientation="vertical">
        <!--    just testing commit -->
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:weightSum="100"
            android:orientation="horizontal"
            android:layout_marginTop="2dp">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="20"

                android:focusable="true"
                android:clickable="true">

                <ImageButton
                    android:id="@+id/lvback"
                    style="@style/TopBack_img" />
            </LinearLayout>

            <TextView
                android:layout_weight="80"
                style="@style/TopHeader_text"
                android:text="@string/s_dna" />


        </LinearLayout>

        <View style="@style/TopLine_View" />

        <ScrollView style="@style/ScrollView">
            <!-- Parent layout starts -->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">


                <LinearLayout style="@style/nLinerLayout">

                    <LinearLayout style="@style/StaricLinLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/dna1" />

                        <TextView style="@style/StaricTextview" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/dna1LV"
                        style="@style/IconLinLayout"

                        >

                        <TextView
                            android:id="@+id/dna1Tv"
                            style="@style/SResponce"
                            android:layout_width="0dp"
                            android:layout_weight="100"
                            android:hint="@string/dna1"

                            />

                        <ImageView style="@style/DownImageView" />

                    </LinearLayout>

                    <View style="@style/nView" />

                </LinearLayout>

                <LinearLayout style="@style/nLinerLayout">

                    <LinearLayout style="@style/StaricLinLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/dna2" />

                        <TextView style="@style/StaricTextview" />
                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/dna2LV"
                        style="@style/IconLinLayout"

                        >

                        <TextView
                            android:id="@+id/dna2Tv"
                            style="@style/SResponce"
                            android:layout_width="0dp"
                            android:layout_weight="100"
                            android:hint="@string/dna2"

                            />

                        <ImageView style="@style/DownImageView" />

                    </LinearLayout>

                    <View style="@style/nView" />

                </LinearLayout>

                <LinearLayout style="@style/nLinerLayout">

                    <TextView
                        style="@style/SLable"
                        android:text="@string/dna3"

                        />

                    <EditText
                        android:id="@+id/dna3Tv"
                        style="@style/SResponce"
                        android:hint="@string/dna3"
                        android:lines="4" />

                    <View style="@style/nView" />

                </LinearLayout>


                <LinearLayout style="@style/nLinerLayout">

                    <LinearLayout style="@style/StaricLinLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/dna4" />

                        <TextView style="@style/StaricTextview" />
                    </LinearLayout>

                    <EditText
                        android:id="@+id/dna4Tv"
                        style="@style/SResponce"
                        android:hint="@string/dna4"
                        android:inputType="textPersonName" />

                    <View style="@style/nView" />

                </LinearLayout>

                <LinearLayout style="@style/nLinerLayout">

                    <LinearLayout style="@style/StaricLinLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/dna5" />

                        <TextView style="@style/StaricTextview" />
                    </LinearLayout>

                    <EditText
                        android:id="@+id/dna5Tv"
                        style="@style/SResponce"
                        android:hint="@string/dna5"
                        android:inputType="textPersonName" />

                    <View style="@style/nView" />

                </LinearLayout>


                <LinearLayout style="@style/nLinerLayout">

                    <LinearLayout style="@style/StaricLinLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/dna6" />

                        <TextView style="@style/StaricTextview" />
                    </LinearLayout>

                    <EditText
                        android:id="@+id/dna6Tv"
                        style="@style/SResponce"
                        android:hint="@string/dna6"
                        android:inputType="number" />

                    <View style="@style/nView" />

                </LinearLayout>

                <LinearLayout style="@style/nLinerLayout">

                    <LinearLayout style="@style/StaricLinLayout">

                        <TextView
                            style="@style/SLable"
                            android:text="@string/dna7" />

                        <TextView style="@style/StaricTextview" />
                    </LinearLayout>

                    <EditText
                        android:id="@+id/dna7Tv"
                        style="@style/SResponce"
                        android:hint="@string/dna7"
                        android:inputType="phone" />

                    <View style="@style/nView" />

                </LinearLayout>

                <LinearLayout style="@style/nLinerLayout">

                    <TextView
                        style="@style/SLable"
                        android:text="@string/dna8"

